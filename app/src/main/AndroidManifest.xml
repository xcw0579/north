        </activity>
        <activity android:name=".Activities.LoginActivity" />
        <activity android:name=".Activities.SignupActivity" />
        <activity android:name=".Activities.FragmentActivity" />
        <activity android:name=".Libraries.volley.VolleyActivity" />
        <activity android:name=".Libraries.testRecycleView.RecycleActivity"></activity><!-- ATTENTION: This was auto-generated to add Google Play services to your project for
     App Indexing.  See https://g.co/AppIndexing/AndroidStudio for more information. -->
